{
  "manifest_version": 3,
  "name": "Save to MyBrain",
  "version": "1.0",
  "description": "Save current website link to MyBrain Dashboard",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}